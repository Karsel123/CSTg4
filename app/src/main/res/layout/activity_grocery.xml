<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Grocery"
    android:background="@color/black">


    <com.google.android.material.navigation.NavigationView
        android:id="@+id/navigationView"
        android:layout_width="413dp"
        android:layout_height="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@color/black">

        <ImageView
            android:id="@+id/profile"
            android:layout_width="43dp"
            android:layout_height="41dp"
            android:layout_marginStart="300dp"
            android:background="@drawable/ic_baseline_person_24"
            android:backgroundTint="#FFFFFF" />

        <ImageView
            android:id="@+id/menu"
            android:layout_width="39dp"
            android:layout_height="48dp"
            android:layout_marginStart="355dp"
            android:background="@drawable/ic_baseline_menu_24"
            android:backgroundTint="#FFFFFF" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="70dp"
            android:layout_height="73dp"
            app:srcCompat="@drawable/c" />

    </com.google.android.material.navigation.NavigationView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="156dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="156dp"
        android:text="Grocery"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/navigationView" />

    <androidx.cardview.widget.CardView
        android:layout_width="399dp"
        android:layout_height="130dp"
        android:layout_marginTop="40dp"
        android:backgroundTint="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/navigationView">


        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="172dp"
            android:layout_height="match_parent"
            app:srcCompat="@drawable/spinach"
            android:scaleType="fitXY"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="270dp"
            android:layout_marginTop="20dp"
            android:text="Spinach"
            android:textSize="19sp"
            android:textStyle="bold"
            android:textColor="#FFFFFF"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="280dp"
            android:layout_marginTop="40dp"
            android:text="Nu 50"
            android:textSize="18sp"
            android:textColor="#FFFFFF"/>

        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="100dp"
            android:layout_height="48dp"
            android:layout_marginStart="260dp"
            android:layout_marginTop="70dp"
            android:background="@drawable/menubutton"
            android:backgroundTint="#3A3B3C"
            android:text="Add"
            android:textColor="#FFFFFF"
            android:textSize="17sp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="406dp"
        android:layout_height="137dp"
        android:layout_marginTop="184dp"
        android:backgroundTint="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/navigationView">


        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="178dp"
            android:layout_height="match_parent"
            app:srcCompat="@drawable/potato"
            android:scaleType="fitXY"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="270dp"
            android:layout_marginTop="20dp"
            android:text="Potatoes"
            android:textSize="19sp"
            android:textStyle="bold"
            android:textColor="#FFFFFF"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="280dp"
            android:layout_marginTop="40dp"
            android:text="Nu 50/kg"
            android:textSize="18sp"
            android:textColor="#FFFFFF"/>

        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="100dp"
            android:layout_height="48dp"
            android:layout_marginStart="260dp"
            android:layout_marginTop="70dp"
            android:background="@drawable/menubutton"
            android:backgroundTint="#3A3B3C"
            android:text="Add"
            android:textColor="#FFFFFF"
            android:textSize="17sp" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="410dp"
        android:layout_height="132dp"
        android:layout_marginTop="480dp"
        android:backgroundTint="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/navigationView">


        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="182dp"
            android:layout_height="match_parent"
            app:srcCompat="@drawable/beans"
            android:scaleType="fitXY"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Green Beans"
            android:layout_marginStart="280dp"
            android:layout_marginTop="20dp"
            android:textColor="#FFFFFF"
            android:textSize="19sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="280dp"
            android:layout_marginTop="40dp"
            android:text="Nu 60/kg"
            android:textSize="18sp"
            android:textColor="#FFFFFF"/>

        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="100dp"
            android:layout_height="48dp"
            android:layout_marginStart="260dp"
            android:layout_marginTop="70dp"
            android:background="@drawable/menubutton"
            android:backgroundTint="#3A3B3C"
            android:text="Add"
            android:textColor="#FFFFFF"
            android:textSize="17sp" />



    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="408dp"
        android:layout_height="130dp"
        android:layout_marginTop="336dp"
        android:backgroundTint="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/navigationView">


        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="173dp"
            android:layout_height="match_parent"
            app:srcCompat="@drawable/eg" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="270dp"
            android:layout_marginTop="20dp"
            android:text="Eggs"
            android:textColor="#FFFFFF"
            android:textSize="19sp"
            android:textStyle="bold" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="280dp"
            android:layout_marginTop="40dp"
            android:text="Nu 20/piece "
            android:textSize="18sp"
            android:textColor="#FFFFFF"/>

        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="100dp"
            android:layout_height="48dp"
            android:layout_marginStart="260dp"
            android:layout_marginTop="70dp"
            android:background="@drawable/menubutton"
            android:backgroundTint="#3A3B3C"
            android:text="Add"
            android:textColor="#FFFFFF"
            android:textSize="17sp"
            tools:ignore="DuplicateClickableBoundsCheck" />



    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="408dp"
        android:layout_height="130dp"
        android:layout_marginTop="336dp"
        android:backgroundTint="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/navigationView">


        <ImageView
            android:id="@+id/img"
            android:layout_width="183dp"
            android:layout_height="131dp"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/tomato" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="270dp"
            android:layout_marginTop="20dp"
            android:text="Tomatoes"
            android:textColor="#FFFFFF"
            android:textSize="19sp"
            android:textStyle="bold" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="280dp"
            android:layout_marginTop="40dp"
            android:text="Nu 50/kg "
            android:textSize="18sp"
            android:textColor="#FFFFFF"/>

        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="100dp"
            android:layout_height="48dp"
            android:layout_marginStart="260dp"
            android:layout_marginTop="70dp"
            android:background="@drawable/menubutton"
            android:backgroundTint="#3A3B3C"
            android:text="Add"
            android:textColor="#FFFFFF"
            android:textSize="17sp" />



    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>